<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.passwordmaker.android" android:versionCode="11" android:versionName="1.1.11">
	<uses-sdk android:minSdkVersion="9" />

	<application android:icon="@drawable/icon" android:label="@string/app_name" android:allowBackup="true" android:allowClearUserData="true">
		<activity android:name=".PasswordMakerProForAndroidActivity"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<intent-filter android:label="Open In PWMP">
				<action android:name="android.intent.action.SEND" />
				<category android:name="android.intent.category.DEFAULT" />
				<data android:mimeType="text/plain" />
			</intent-filter>
		</activity>
		<activity android:name=".PasswordMakerEditProfile"
			android:label="@string/ChangeProfile">
		</activity>
		<activity android:name=".PasswordMakerEditFavorites"
			android:label="@string/EditFavorites">
		</activity>
 <receiver 
android:name="org.passwordmaker.android.EmmaInstrument.SMSInstrumentedReceiver"> 
   <intent-filter> 
     <action android:name="edu.gatech.m3.emma.COLLECT_COVERAGE"/> 
   </intent-filter> 
     </receiver>
	<activity android:label="EmmaInstrumentationActivity"  android:name="org.passwordmaker.android.EmmaInstrument.InstrumentedActivity" />
	</application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <instrumentation    android:handleProfiling="true"    android:label="EmmaInstrumentation"  android:name="org.passwordmaker.android.EmmaInstrument.EmmaInstrumentation" android:targetPackage="org.passwordmaker.android"/>
</manifest>