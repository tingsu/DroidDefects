+$the_cow = <<"EOC";
+   $thoughts        \\
+    $thoughts        \\
+     $thoughts       _\\^
+      $thoughts    _- $eyes\\
+           \\---- \\______
+            $tongue   \\       )\\
+                ||-----||  \\
+                ||     ||
+EOC
